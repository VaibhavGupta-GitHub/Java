//OOPS Start

class Pen {
    String colour ;
    String Material;
    String type;

    public void Write () {
        System.out.println("Written");
    }
}

public class OOPS {
    public static void main(String[] args) {
        Pen pen1 = new Pen();
        pen1.type = "gel";
        pen1.colour = "black";
        pen1.Material = "platic";

        pen1.Write();
    }
}
